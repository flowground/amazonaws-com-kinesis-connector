        "requestBody": {
            "description": "Represents the input for <code>CreateStream</code>.",
            "properties": {
                "ShardCount": {
                    "description": "<p>The number of shards that the stream will use. The throughput of the stream is a function of the number of shards; more shards are required for greater provisioned throughput.</p> <p>DefaultShardLimit;</p>",
                    "maximum": 100000,
                    "minimum": 1,
                    "type": "integer"
                },
                "StreamName": {
                    "description": "A name to identify the stream. The stream name is scoped to the AWS account used by the application that creates the stream. It is also scoped by AWS Region. That is, two streams in two different AWS accounts can have the same name. Two streams in the same AWS account but in two different Regions can also have the same name.",
                    "maxLength": 128,
                    "minLength": 1,
                    "pattern": "[a-zA-Z0-9_.-]+",
                    "type": "string"
                }
            },
            "required": [
                "StreamName",
                "ShardCount"
            ],
            "type": "object"
        },
